exports.adminMiddleware = void 0;
const adminMiddleware = (req, res, next) => {
    if (!req.user || !req.user.isAdmin) {
        return res.status(403).json({ message: 'Admin access required' });
    }
    return next();
};
exports.adminMiddleware = adminMiddleware;
